<% if is_authorized_user %>
	<input type="button" onclick="tableToExcel('testTable', 'W3C Example Table')" value="Export to Excel">
	<table id='testTable'>
	<% @users.each do |user| %>
		<% user.members.each do |member|%>
			
			<tr>
				<td><%= member.name%> </td>
				<td><%= member.birthday %> </td>
				<td> <%= member.person_id %> </td>
				
			<tr>
				<td>.</td>
				<td>.</td>
				<td>.</td>
			</tr>
		<% end %>
	<% end %>
	</table>
<% end %>